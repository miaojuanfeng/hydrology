<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="gov.gz.hydrology.mapper.write.CacheRiverTimeDao" >
	<resultMap id="BaseResultMap" type="gov.gz.hydrology.entity.write.CacheRiverTime" >
		<id column="stcd" property="stcd" jdbcType="VARCHAR" />
        <result column="tm" property="tm"/>
        <result column="z" property="z"/>
	</resultMap>

    <!-- 批量设备数据 -->
    <insert id="insertBatch" parameterType="java.util.List">
        insert into t_device_data
        (device_id,data_type,device_data,inserter,insert_time,updater,update_time)
        values
        <foreach collection="list" item="item" index="index" separator=",">
            (#{item.deviceId},#{item.dataType},#{item.deviceData},#{item.inserter},#{item.insertTime},#{item.updater},#{item.updateTime})
        </foreach>
    </insert>
</mapper>